tests
testSequenceOfCollectShifts

	| res |
	res := [
	       | v w |
	       v := (1 to: 10) asArray collectShift: #doubled.
	       w := (11 to: 20) asArray collectShift: #doubled.
	       {
		       v.
		       w } ] reset.

	self
		assert: res
		equals:
			#( #( #( 2 22 ) #( 2 24 ) #( 2 26 ) #( 2 28 ) #( 2 30 ) #( 2 32 )
			      #( 2 34 ) #( 2 36 ) #( 2 38 ) #( 2 40 ) )
			   #( #( 4 22 ) #( 4 24 ) #( 4 26 ) #( 4 28 ) #( 4 30 ) #( 4 32 )
			      #( 4 34 ) #( 4 36 ) #( 4 38 ) #( 4 40 ) )
			   #( #( 6 22 ) #( 6 24 ) #( 6 26 ) #( 6 28 ) #( 6 30 ) #( 6 32 )
			      #( 6 34 ) #( 6 36 ) #( 6 38 ) #( 6 40 ) )
			   #( #( 8 22 ) #( 8 24 ) #( 8 26 ) #( 8 28 ) #( 8 30 ) #( 8 32 )
			      #( 8 34 ) #( 8 36 ) #( 8 38 ) #( 8 40 ) )
			   #( #( 10 22 ) #( 10 24 ) #( 10 26 ) #( 10 28 ) #( 10 30 )
			      #( 10 32 ) #( 10 34 ) #( 10 36 ) #( 10 38 ) #( 10 40 ) )
			   #( #( 12 22 ) #( 12 24 ) #( 12 26 ) #( 12 28 ) #( 12 30 )
			      #( 12 32 ) #( 12 34 ) #( 12 36 ) #( 12 38 ) #( 12 40 ) )
			   #( #( 14 22 ) #( 14 24 ) #( 14 26 ) #( 14 28 ) #( 14 30 )
			      #( 14 32 ) #( 14 34 ) #( 14 36 ) #( 14 38 ) #( 14 40 ) )
			   #( #( 16 22 ) #( 16 24 ) #( 16 26 ) #( 16 28 ) #( 16 30 )
			      #( 16 32 ) #( 16 34 ) #( 16 36 ) #( 16 38 ) #( 16 40 ) )
			   #( #( 18 22 ) #( 18 24 ) #( 18 26 ) #( 18 28 ) #( 18 30 )
